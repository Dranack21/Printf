### COMPIL #####################################################################

CC = cc 
CFLAGS = -Wall -Wextra -Werror
NAME = libftprintf.a

### SRCS #######################################################################

SRCS = 

### OBJS #######################################################################

OBJS = $(SRCS:.c=.o)
%.o: %.c
	$(CC) $(CFLAGS) -c $< -o $@

### RULES #######################################################################

all : NAME

$(NAME) : $(OBJS)
	$(CC) $(CFLAGS)

clean : 
	rm -f $(OBJS)

fclean : clean
	rm -f $(NAME)

re: 
	fclean all

.PHONY : re fclean all make